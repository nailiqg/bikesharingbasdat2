--
-- Name: jumlah_denda(); Type: FUNCTION; Schema: bikesharing; Owner: db2018044
--

CREATE FUNCTION bikesharing.jumlah_denda() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
begin 
if (TG_OP='UPDATE') then
if sum(new.datetime_kembali-old.datetime_pinjam) > 10 then
new.jam=sum(new.datetime_kembali-old.datetime_pinjam)-10;
update peminjaman set denda= 50000*new.jam;
elseif sum(datetime_kembali-datetime_pinjam) > 24 then
update peminjaman set denda=3000000;
end if;
return new;
end if;
end;
$$;


