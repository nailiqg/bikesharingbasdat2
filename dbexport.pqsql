CREATE FUNCTION bikesharing.buat_laporan() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
begin
if (TG_OP='UPDATE') then 
if sum(datetime_kembali-datetime_pinjam) > 24 then
insert into laporan values(new.id_laporan,new.no_kartu_anggota, new.datetime_pinjam,new.nomor_sepeda, new.id_stasiun, new.status);
end if;
return new;
end if;
end;
$$;


